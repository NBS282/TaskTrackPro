<body>
<Routes/>
<script>
  window.initResourceCalendar = function (events) {
    const calendarEl = document.getElementById('resourceCalendar');
    if (window._resourceCal) window._resourceCal.destroy();
    window._resourceCal = new FullCalendar.Calendar(calendarEl, {
      initialView: 'dayGridMonth',
      headerToolbar: {left: 'prev,next today', center: 'title', right: ''},
      height: 600,
      events: events
    });
    window._resourceCal.render();
  };
</script>

<script src="_framework/blazor.web.js"></script>
